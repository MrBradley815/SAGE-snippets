  "SAGE radio selection": {
    "prefix": "form:radio",
    "body": [
      "<div class=\"form-floating mb-3\">",
      "\t<p class=\"mb-1\">Is Superman stronger than Hulk?</p>",
      "\t<div class=\"form-check\">",
      "\t\t<input type=\"radio\" class=\"form-check-input\" id=\"radio1\" name=\"Who is stronger?\" value=\"Yes\" />",
      "\t\t<label class=\"form-check-label\" for=\"radio1\">Yes</label>",
      "\t</div>",
      "\t<div class=\"form-check\">",
      "\t\t<input type=\"radio\" class=\"form-check-input\" id=\"radio2\" name=\"Who is stronger?\" value=\"No\" />",
      "\t\t<label class=\"form-check-label\" for=\"radio2\">No</label>",
      "\t</div>",
      "</div>"
    ],
    "description": "Radio selection for a SAGE custom form"
  },
